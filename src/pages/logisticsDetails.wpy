<style>
  page{
    height: 100%;
  }
  .mainInAll{
    background: url('data:image/gif;base64,') no-repeat center center #fff;
    background-attachment: fixed;
    background-size: 40rpx;
    height: 100%;
  }
  .main{
    background:#f9fafb;
    opacity: 0;
  }
  .mainIn {
    -webkit-animation: mainIn .5s .1s ease both
  }
  @keyframes mainIn {
    0% {
      opacity: 0
    }
    100% {
      opacity: 1
    }
  }
  @-webkit-keyframes mainIn {
    0% {
      opacity: 0
    }
    100% {
      opacity: 1
    }
  }
  .header{
    background: #fff;
    padding: 20rpx;
    display: flex;
  }
  .left{
    width: 150rpx;
    height: 90rpx;
    margin-right: 20rpx;
  }
  .left image{
    width: 100%;
    height: 100%;
  }
  .right{
    font-size: 30rpx;
    line-height: 50rpx;
  }
  .fz{
    border:1px solid #5c5c5c;
    border-radius:10rpx;
    padding:6rpx 14rpx;
    margin-left: 10rpx;
  }
  .header1 .name{
    color: #26ae60;
  }
  .mainwl{
    background: #fff;
  }
  .mainwl_bt{
    font-size: 30rpx;
    padding: 20rpx;
    margin-top: 20rpx;
    background: #fff;
    border-bottom: 1px solid #ededed;
  }
  .mainwl_li{
    color: #9c9c9c;
    padding: 20rpx;
    border-bottom: 1px solid #ededed;
  }
  .mainwl_li_txt_tou{
    font-size: 28rpx;
    line-height: 48rpx;
  }
  .mainwl_li_txt_sj{
    font-size: 26rpx;
    margin-top: 20rpx;
  }
  .mainwl_li:first-child{
    color: #26ae60;
  }
  .sm{
    text-align: center;
    padding:80rpx 0;
  }
</style>

<template>
  <view class="mainInAll">
    <view class="{{LoadingState ? 'main mainIn' : 'main'}}">
      <view class="headerall" wx:if="{{waybillNumber}}">
        <view class="header">
          <view class="header1 left">
            <image src="{{img}}" mode="aspectFit"></image>
          </view>
          <view class="header1 right">
            <view class="header2">物流公司：<span class="name">{{logistics}}</span></view>
            <view class="header2">运单编号：<span class="nu">{{waybillNumber}}</span><span class="fz" @tap="copy">复制</span></view>
          </view>
        </view>
        <view class="mainwl_bt">订单跟踪</view>
        <view class="mainwl">
          <view class="sm" wx:if="{{!isHave}}">运单编号不正确或还没有收录！</view>
          <view class="mainwl_li" wx:for="{{progress}}" wx:key="{{index}}">
            <view class="mainwl_li_txt_tou">{{item.context}}</view>
            <view class="mainwl_li_txt_sj">{{item.time}}</view>
          </view>
        </view>
      </view>
      <view class="sm" wx:else>运单编号不存在！</view>
    </view>
  </view>
</template>

<script>
  import wepy from 'wepy';
  export default class Index extends wepy.page {
    config = {
      navigationBarTitleText: '查看物流'
    };
    data = {
      LoadingState: 0,
      categoryId: '',
      orderId: '',
      img: '',
      logistics: '',
      waybillNumber: '',
      progress: [],
      isHave: 0
    };
    methods = {
      copy() {
        let me = this;
        if (!me.waybillNumber) {
          wepy.showToast({title: '没有内容', image: '../assets/img/toast-icon.png', mask: 1, duration: 1000});
          return;
        }
        wepy.setClipboardData({
          data: me.waybillNumber,
          success: function(res) {
            wepy.showToast({title: '复制成功', mask: 1, duration: 1000});
          }
        });
      }
    };
    onLoad(options) {
      let me = this;
      me.orderId = options.orderId;
      me.categoryId = options.categoryId;
      me.$apply();
      wepy.request({
        url: `${me.$parent.APIServer}/guide/order/express/${me.orderId}`,
        method: 'GET',
        header: {
          'accessToken': me.$parent.globalData.accessToken
        },
        success: function (data) {
          let result = data.data.data;
          if (result) {
            me.LoadingState = 1;
            me.img = JSON.parse(result.activity.properties).filePaths[0];
            me.logistics = result.expressName;
            me.waybillNumber = result.expressNo;
            me.progress = result.progress || [];
            me.isHave = result.progress || 0;
            me.$apply();
          }
        }
      });
    }
  }
</script>
